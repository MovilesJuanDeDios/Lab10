<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.casca.lab05">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".Activity.Navigation"
            android:label="@string/title_activity_navigation"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.Login"
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name=".Activity.Payment"
            android:label="@string/title_activity_payment"
            android:screenOrientation="portrait"></activity>
        <activity
            android:name=".Activity.Carrito"
            android:label="@string/title_activity_carrito"
            android:screenOrientation="portrait"></activity>
        <activity android:name=".Activity.Registro">

        </activity>
    </application>

</manifest>